  <script>
// Horizontal Scroll-Jacking for Features Section
class HorizontalScrollJacker {
  constructor() {
    this.section = document.querySelector('.section_features');
    this.wrapper = document.querySelector('.features-sticky-wrapper');
    this.scrollContainer = document.querySelector('.features-horizontal-scroll');
    this.cards = document.querySelectorAll('.feature-card-large');

    if (!this.section || !this.scrollContainer || this.cards.length === 0) {
      return;
    }

    this.cardCount = this.cards.length;
    this.ticking = false;

    this.init();
  }

  init() {
    // Calculate max translation distance dynamically
    const containerWidth = this.scrollContainer.offsetWidth;
    const viewportWidth = window.innerWidth;
    this.maxTranslate = containerWidth - viewportWidth;

    // Set section height: 400vh for slower, smoother scrolling through 5 cards
    this.section.style.height = '400vh';

    // Set up smooth scroll handling with requestAnimationFrame
    window.addEventListener('scroll', () => {
      if (!this.ticking) {
        window.requestAnimationFrame(() => {
          this.handleScroll();
          this.ticking = false;
        });
        this.ticking = true;
      }
    });

    // Recalculate on resize
    window.addEventListener('resize', this.debounce(() => {
      const containerWidth = this.scrollContainer.offsetWidth;
      const viewportWidth = window.innerWidth;
      this.maxTranslate = containerWidth - viewportWidth;
      this.handleScroll();
    }, 250));

    // Mouse drag to scroll (optional)
    this.setupDragScroll();
  }

  handleScroll() {
    const rect = this.section.getBoundingClientRect();
    const sectionTop = rect.top;
    const viewportHeight = window.innerHeight;
    const sectionHeightPx = this.section.offsetHeight;

    // Active range: from when section reaches top until it exits viewport
    if (sectionTop <= 0 && sectionTop > -(sectionHeightPx - viewportHeight)) {
      // Calculate scroll progress (0 to 1)
      const scrolled = Math.abs(sectionTop);
      const scrollRange = sectionHeightPx - viewportHeight;
      const progress = Math.min(Math.max(scrolled / scrollRange, 0), 1);

      // Direct 1:1 mapping for consistent, complete scrolling in both directions
      // CSS transition (0.15s ease-out) provides smooth motion
      const translateX = -(progress * this.maxTranslate);
      this.scrollContainer.style.transform = `translateX(${translateX}px)`;
    } else if (sectionTop > 0) {
      // Before section - reset to start
      this.scrollContainer.style.transform = 'translateX(0)';
    }
    // After section exits - keep final position
  }

  setupDragScroll() {
    let isDown = false;
    let startX;
    let currentTranslate = 0;

    this.scrollContainer.addEventListener('mousedown', (e) => {
      isDown = true;
      this.scrollContainer.style.cursor = 'grabbing';
      startX = e.pageX;
      const transform = this.scrollContainer.style.transform;
      currentTranslate = transform ? parseFloat(transform.replace(/[^\-\d.]/g, '')) : 0;
    });

    this.scrollContainer.addEventListener('mouseleave', () => {
      isDown = false;
      this.scrollContainer.style.cursor = 'default';
    });

    this.scrollContainer.addEventListener('mouseup', () => {
      isDown = false;
      this.scrollContainer.style.cursor = 'default';
    });

    this.scrollContainer.addEventListener('mousemove', (e) => {
      if (!isDown) return;
      e.preventDefault();
      const x = e.pageX;
      const walk = (x - startX) * 1.5;
      const newTranslate = Math.max(Math.min(currentTranslate + walk, 0), -this.maxTranslate);
      this.scrollContainer.style.transform = `translateX(${newTranslate}px)`;
    });
  }

  debounce(func, wait) {
    let timeout;
    return function executedFunction(...args) {
      const later = () => {
        clearTimeout(timeout);
        func(...args);
      };
      clearTimeout(timeout);
      timeout = setTimeout(later, wait);
    };
  }
}

// Initialize horizontal scroll-jacking for features section
document.addEventListener('DOMContentLoaded', () => {
  new HorizontalScrollJacker();
});
</script>
  <script>
// Manifesto Text Reveal Animation
document.addEventListener('DOMContentLoaded', () => {
  const manifestoSection = document.querySelector('.section_manifesto');
  const manifestoWords = document.querySelectorAll('.manifesto-word');

  if (!manifestoSection || manifestoWords.length === 0) {
    return;
  }

  // Single, optimized ScrollTrigger for word reveal
  ScrollTrigger.create({
    trigger: manifestoSection,
    start: 'top 90%', // Start when section is just entering viewport
    end: 'bottom 30%', // End earlier to ensure completion before section exits
    scrub: 2, // Smoother, less reactive scrubbing
    onUpdate: (self) => {
      const progress = self.progress;
      const totalWords = manifestoWords.length;

      manifestoWords.forEach((word, index) => {
        // Calculate this word's reveal range (0 to 1)
        // Scale progress to ensure completion (multiply by 1.1 and clamp)
        const scaledProgress = Math.min(progress * 1.1, 1);
        const wordStart = index / totalWords;
        const wordEnd = (index + 1) / totalWords;

        // Add overlap for smoother transitions (words blend into each other)
        const overlapFactor = 0.15; // Increased for smoother blending
        const adjustedStart = Math.max(0, wordStart - overlapFactor);
        const adjustedEnd = Math.min(1, wordEnd + overlapFactor);

        if (scaledProgress <= adjustedStart) {
          // Before this word's reveal range
          word.style.opacity = '0.3';
        } else if (scaledProgress >= adjustedEnd) {
          // After this word's reveal range
          word.style.opacity = '1';
        } else {
          // Within this word's reveal range - calculate smooth transition
          const localProgress = (scaledProgress - adjustedStart) / (adjustedEnd - adjustedStart);
          // Ease the opacity change for smoother animation
          const easedProgress = gsap.utils.clamp(0, 1, localProgress);
          word.style.opacity = 0.3 + (easedProgress * 0.7);
        }
      });
    }
  });

  // Optional: Add a subtle hover effect for interactivity
  manifestoWords.forEach(word => {
    word.style.transition = 'opacity 0.3s ease, transform 0.2s ease';

    word.addEventListener('mouseenter', () => {
      if (word.style.opacity !== '1') {
        word.style.transform = 'scale(1.05)';
      }
    });

    word.addEventListener('mouseleave', () => {
      word.style.transform = 'scale(1)';
    });
  });
});
</script>
